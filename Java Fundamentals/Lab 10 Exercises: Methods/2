//Write a method that receives a single string and prints the count of the vowels. 
//Use appropriate name for the method.

import java.util.Scanner;

public class zadachi {
	
	public static void printSmallestNumber(String arg) {
		arg.toCharArray();
		int vowels = 0;
		for (int i = 0; i < arg.length(); i++) {
			if (arg.charAt(i) == 'a' || arg.charAt(i) == 'o'|| arg.charAt(i) == 'u' 
					|| arg.charAt(i) == 'i' || arg.charAt(i) == 'e') {
				vowels++;
			}
		}
		System.out.print(vowels);
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String word = scan.nextLine();
		word = word.toLowerCase();
		printSmallestNumber(word);
	}	
}
