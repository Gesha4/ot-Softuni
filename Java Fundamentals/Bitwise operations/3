
//Write a program that prints the bit at position p of a number.

import java.util.Scanner;

public class zadachi {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int p = scan.nextInt();
		int k = n >> p;
		if ((k & 1) == 1) {
			System.out.print("1");
		} else {
		System.out.print("0");
		}
	}
}
